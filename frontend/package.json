{
  "name": "polimane",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build || true",
    "lint:oxlint": "oxlint . --fix --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "update-deps": "npm-check-updates -u",
    "svgo": "svgo --config ./svgo.js",
    "deploy": "wrangler pages deploy ./dist --branch main --project-name $PROJECT_NAME"
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
    "@atlaskit/pragmatic-drag-and-drop-auto-scroll": "^2.1.1",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.1.0",
    "@iconify-json/eva": "^1.2.1",
    "@sentry/vite-plugin": "^3.5.0",
    "@sentry/vue": "^9.37.0",
    "@types/bun": "^1.2.18",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.5.0",
    "konva": "^9.3.22",
    "lightningcss": "^1.30.1",
    "normalize.css": "^8.0.1",
    "npm-run-all2": "^8.0.4",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "postcss-nesting": "^13.0.2",
    "rambda": "^10.3.0",
    "typescript": "^5.8.3",
    "ulid": "^3.0.1",
    "unplugin-icons": "^22.1.0",
    "vite": "^7.0.4",
    "vue": "^3.5.17",
    "vue-component-type-helpers": "^3.0.1",
    "vue-konva": "^3.2.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.1",
    "wrangler": "^4.24.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.1.0",
    "@vue/devtools-api": "^7.7.7",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-oxlint": "^1.6.0",
    "eslint-plugin-vue": "~10.3.0",
    "npm-check-updates": "^18.0.1",
    "oxlint": "^1.6.0",
    "svgo": "^4.0.0",
    "vite-plugin-vue-devtools": "^7.7.7"
  }
