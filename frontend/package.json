{
  "name": "polimane",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build || true",
    "lint:oxlint": "oxlint . --fix --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "update-deps": "npm-check-updates -u",
    "svgo": "svgo --config ./svgo.js",
    "svgo:assets": "bun svgo -rf src/assets",
    "deploy": "wrangler pages deploy ./dist --branch main --project-name $PROJECT_NAME"
  },
  "dependencies": {
    "@iconify-json/eva": "^1.2.3",
    "@sentry/vite-plugin": "^4.5.0",
    "@sentry/vue": "^10.21.0",
    "@types/bun": "^1.3.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.9.0",
    "colorjs.io": "^0.5.2",
    "jspdf": "^3.0.3",
    "lightningcss": "^1.30.2",
    "normalize.css": "^8.0.1",
    "npm-run-all2": "^8.0.4",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "rambda": "^10.3.2",
    "svg2pdf.js": "^2.6.0",
    "typescript": "^5.9.3",
    "ulid": "^3.0.1",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.1.11",
    "vue": "^3.5.22",
    "vue-component-type-helpers": "^3.1.1",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.1",
    "wrangler": "^4.44.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.5.0",
    "@vue/devtools-api": "^8.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-oxlint": "^1.23.0",
    "eslint-plugin-vue": "~10.5.1",
    "npm-check-updates": "^19.1.1",
    "oxlint": "^1.23.0",
    "svgo": "^4.0.0",
    "vite-plugin-vue-devtools": "^8.0.3"
  }
}
